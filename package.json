{
  "name": "gatsby-starter-lingui",
  "private": true,
  "description": "Gatsby lingui starter",
  "version": "1.1.3",
  "author": "hello@webappsolutions.de",
  "repository": "hupe1980/gatsby-i18n",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn lingui extract --clean && NODE_ENV=production gatsby build",
    "dev": "yarn clean && CHOKIDAR_USEPOLLING=1 NODE_ENV=development gatsby develop --port \"${PORT:-5000}\" --open -H 0.0.0.0",
    "serve": "gatsby serve",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "clean": "git clean -fdxq 'src/locales/**/*.js' './src/locales/_build' '.cache' 'public'"
  },
  "dependencies": {
    "@lingui/react": "^2.4.0",
    "@wapps/gatsby-plugin-lingui": "^1.1.3",
    "gatsby": "^2.0.9",
    "gatsby-plugin-manifest": "^2.0.9",
    "gatsby-plugin-offline": "^2.0.9",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-source-filesystem": "^2.0.9",
    "react": "^16.8.0-alpha.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@lingui/cli": "^2.4.0"
  },
  "gitHead": "1bf52e4b2a459af17526d03ebaf34cb1a87b5ab4"
}
